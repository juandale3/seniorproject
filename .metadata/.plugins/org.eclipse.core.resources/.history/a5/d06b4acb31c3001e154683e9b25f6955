} state;

void state_machine() {
	bool error = false;
	switch(state) {
		case START:
			break;

		case VAC_ACHIEVMENT_TEST:


			break;
		case WARM_UP:
			break;
		case LOAD_TEST:
			break;
		case OPERATION_TEST:
			break;
		case ULTIMATE_MEASURE_TEST:
			break;
	}
}
