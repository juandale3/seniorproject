enum states {
	START,
	VAC_ACHIEVMENT_TEST,
	WARM_UP,
	LOAD_TEST,
	OPERATION_TEST,
	ULTIMATE_MEASURE_TEST

} state;

void state_machine(enum events event) {
	switch(state) {
		case START:
			break;

		case VAC_ACHIEVEMENT_TEST:

			break;
		case WARM_UP:
			break;
		case LOAD_TEST:
			break;
		case OPERATION_TEST:
			break;
	}
}
