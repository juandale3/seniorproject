#include "main.h"
#include "pumpInterface.h"

#define TRANSMIT_SIZE 16

uint8_t tx_buffer[TRANSMIT_SIZE];


void accessPumpData(UART_HandleTypeDef *huart){


	HAL_UART_Transmit(&huart3, (uint8_t *) tx_buffer, 15, HAL_MAX_DELAY);
	return;
}
