#ifndef INC_STATES_H_
#define INC_STATES_H_

  enum State {
      START,
	  VAC_ACHIEVMENT_TEST_INIT,
      VAC_ACHIEVMENT_TEST,
	  WARM_UP_INIT,
      WARM_UP,
      LOAD_TEST_INIT,
	  LOAD_TEST,
      OPERATION_TEST_INIT,
	  OPERATION_TEST,
	  ULTIMATE_MEASURE_TEST_INIT,
      ULTIMATE_MEASURE_TEST,
      IDLE,
      FAIL_STATE,
      STOP
  };


#endif /* INC_STATES_H_ */
