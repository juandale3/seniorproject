#include "main.h"
#include "pumpInterface.h"

#define TRANSMIT_SIZE 16

uint8_t tx_command[TRANSMIT_SIZE];


void accessPumpData(UART_HandleTypeDef *huart){


	HAL_UART_Transmit(&huart3, &tx_command[0], TRANSMIT_SIZE, HAL_MAX_DELAY);
	return;
}
